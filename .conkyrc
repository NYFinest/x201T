background no
out_to_console yes
out_to_x no
update_interval 1.0
total_run_times 0
use_spacer none

TEXT
^i(/home/edwin/.icons/pc.xbm) /root ${fs_used_perc /}% /home ${fs_used_perc /home}% /var ${fs_used_perc /var}% * ^i(/home/edwin/.icons/temp.xbm) ${hwmon temp 1}C * ^i(/home/edwin/.icons/arch1.xbm) ${uptime} * ^i(/home/edwin/.icons/arrow_down2.xbm) ${downspeedf wlan0} Kb/s ^i(/home/edwin/.icons/arrow_up2.xbm) ${upspeedf wlan0}Kb/s * ^i(/home/edwin/.icons/battery_vert3.xbm) ${battery_percent}% * ^i(/home/edwin/.icons/cpu.xbm) ${cpu cpu1}% ${cpu cpu2}% ${cpu cpu3}% ${cpu cpu4}% * ^i(/home/edwin/.icons/memory.xbm) ${memperc}% * ^i(/home/edwin/.icons/clock2.xbm) ${time %a %b %d %I:%M%P}
